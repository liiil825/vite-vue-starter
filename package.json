{
  "name": "vite-vue-starter",
  "type": "module",
  "version": "0.0.0",
  "private": false,
  "keywords": [
    "chatgpt-web",
    "chatgpt",
    "chatbot",
    "vue"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview"
  },
  "dependencies": {
    "@types/crypto-js": "^4.1.1",
    "@vueuse/core": "^10.1.0",
    "crypto-js": "^4.1.1",
    "naive-ui": "^2.34.3",
    "pinia": "^2.0.35",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.5",
    "@iconify/vue": "^4.1.1",
    "@intlify/unplugin-vue-i18n": "^0.10.0",
    "@types/node": "^18.15.13",
    "@vitejs/plugin-vue": "^4.1.0",
    "autoprefixer": "^10.4.14",
    "eslint-plugin-prettier": "^4.2.1",
    "less": "^4.1.3",
    "lint-staged": "^13.2.1",
    "postcss": "^8.4.23",
    "prettier": "^2.8.7",
    "tailwindcss": "^3.3.1",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vue-tsc": "^1.2.0"
  },
  "lint-staged": {
    "*.{ts,tsx,vue}": [
      "pnpm lint:fix"
    ]
  }
